{
    "require": {
        "amazon-php/sp-api-sdk": "^4.0",
        "equip/assist": "^1.6.0",
        "equip/framework": "^2.1",
        "josegonzalez/dotenv": "^2.0",
        "league/plates": "^3.3",
        "zendframework/zend-diactoros": "^1.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
